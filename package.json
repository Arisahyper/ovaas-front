{
  "name": "ovaas-front",
  "version": "0.1.0",
  "description": "OVaaS Front-end use Vue.js v3",
  "license": "Apache-2.0",
  "repository": "github:ovaas/ovaas-front",
  "scripts": {
    "dev": "vite",
    "dev:debug": "vite --debug",
    "build": "cross-env NODE_ENV=production vite build",
    "prebuild": "test \"$CI\" = true && npx pnpm install -r --store=node_modules/.pnpm-store || echo skiping pnpm install",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@iconify/iconify": "2.0.0-rc.1",
    "@vueuse/core": "4.0.0-rc.4",
    "axios": "^0.20.0",
    "vue": "3.0.3",
    "vue-i18n": "next",
    "vue-router": "next"
  },
  "devDependencies": {
    "@antfu/eslint-config-vue": "^0.4.3",
    "@iconify/json": "^1.1.265",
    "@purge-icons/generated": "^0.4.1",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@vue/compiler-sfc": "^3.0.3",
    "@vuedx/typescript-plugin-vue": "^0.2.4-0",
    "autoprefixer": "^10.0.4",
    "cross-env": "^7.0.2",
    "eslint": "^7.14.0",
    "tailwindcss": "^2.0.1",
    "typescript": "^4.1.2",
    "vite": "1.0.0-rc.13",
    "vite-plugin-components": "^0.4.1",
    "vite-plugin-purge-icons": "^0.4.5",
    "vite-plugin-voie": "^0.4.0",
    "voie-pages": "^0.4.0",
    "vue-eslint-parser": "^7.1.1"
  },
  "pnpm": {
    "overrides": {
      "postcss": "8",
      "vue-router": "next"
    }
  },
  "eslintConfig": {
    "extends": "@antfu/eslint-config-vue",
    "rules": {
      "no-unused-vars": "off",
      "no-alert": "off",
      "@typescript-eslint/no-unused-vars": "off"
    }
  }
}
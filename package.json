{
  "name": "ovaas-front",
  "version": "0.1.0",
  "description": "OVaaS Front-end use Vue.js v3",
  "license": "Apache-2.0",
  "repository": "github:ovaas/ovaas-front",
  "scripts": {
    "dev": "vite",
    "dev:debug": "vite --debug",
    "build": "cross-env NODE_ENV=production vite-ssg build --script async"
  },
  "dependencies": {
    "@vueuse/core": "^4.0.4",
    "axios": "^0.21.1",
    "mitt": "^2.1.0",
    "nprogress": "^0.2.0",
    "vue": "^3.0.5",
    "vue-i18n": "^9.0.0-rc.1",
    "vue-router": "^4.0.2"
  },
  "devDependencies": {
    "@antfu/eslint-config-vue": "^0.4.3",
    "@iconify/json": "^1.1.282",
    "@purge-icons/generated": "^0.5.0",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@vitejs/plugin-vue": "^1.0.4",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/server-renderer": "^3.0.5",
    "@vuedx/typescript-plugin-vue": "^0.4.1",
    "autoprefixer": "^10.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.17.0",
    "postcss": "^8.2.3",
    "postcss-nested": "^5.0.3",
    "presite": "^2.1.1",
    "tailwindcss": "^2.0.2",
    "typescript": "^4.1.3",
    "vite": "^2.0.0-beta.12",
    "vite-plugin-components": "^0.6.3",
    "vite-plugin-icons": "^0.2.1",
    "vite-plugin-purge-icons": "^0.5.0",
    "vite-plugin-voie": "^0.7.1",
    "vite-ssg": "^0.3.0",
    "voie-pages": "^0.4.0",
    "vue-eslint-parser": "^7.3.0"
  },
  "eslintConfig": {
    "extends": "@antfu/eslint-config-vue",
    "ignorePatterns": [
      "dist/*",
      "**/vendor/*.js"
    ],
    "rules": {
      "no-unused-vars": "off",
      "no-alert": "off",
      "no-cond-assign": [
        2,
        "except-parens"
      ],
      "@typescript-eslint/no-unused-vars": "off"
    }
  }
}

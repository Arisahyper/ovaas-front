{
  "name": "ovaas-front",
  "version": "0.1.0",
  "description": "OVaaS Front-end use Vue.js v3",
  "license": "Apache-2.0",
  "repository": "github:ovaas/ovaas-front",
  "scripts": {
    "dev": "vite",
    "dev:debug": "vite --debug",
    "build": "cross-env NODE_ENV=production vite-ssg build --script async",
    "build:ssg": "cross-env NODE_ENV=production vite build && esno scripts/ssg.ts"
  },
  "dependencies": {
    "@vueuse/core": "^4.0.1",
    "axios": "^0.21.1",
    "mitt": "^2.1.0",
    "nprogress": "^0.2.0",
    "vue": "^3.0.4",
    "vue-i18n": "9.0.0-beta.8",
    "vue-router": "^4.0.2"
  },
  "devDependencies": {
    "@antfu/eslint-config-vue": "^0.4.3",
    "@iconify/json": "^1.1.278",
    "@purge-icons/generated": "^0.4.1",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@vue/compiler-sfc": "^3.0.4",
    "@vue/server-renderer": "^3.0.4",
    "@vuedx/typescript-plugin-vue": "^0.4.0",
    "autoprefixer": "^10.1.0",
    "chromium": "^3.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.16.0",
    "esno": "^0.3.0",
    "postcss-nested": "^5.0.3",
    "presite": "^2.1.1",
    "tailwindcss": "^2.0.2",
    "typescript": "^4.1.3",
    "vite": "^1.0.0-rc.13",
    "vite-plugin-components": "^0.5.6",
    "vite-plugin-icons": "^0.1.0",
    "vite-plugin-purge-icons": "^0.4.5",
    "vite-plugin-voie": "^0.5.0",
    "vite-ssg": "^0.1.0",
    "voie-pages": "^0.4.0",
    "vue-eslint-parser": "^7.3.0"
  },
  "pnpm": {
    "overrides": {
      "postcss": "8",
      "vue-router": "next"
    }
  },
  "eslintConfig": {
    "extends": "@antfu/eslint-config-vue",
    "ignorePatterns": [
      "dist/*",
      "**/vendor/*.js"
    ],
    "rules": {
      "no-unused-vars": "off",
      "no-alert": "off",
      "@typescript-eslint/no-unused-vars": "off"
    }
  }
}
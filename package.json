{
  "name": "ovaas-front",
  "version": "1.0.3",
  "description": "OVaaS Front-end use Vue.js v3",
  "license": "Apache-2.0",
  "repository": "github:ovaas/ovaas-front",
  "scripts": {
    "dev": "vite",
    "dev:debug": "vite --debug",
    "serve": "vite preview",
    "build": "cross-env-shell NODE_ENV=production vite-ssg build",
    "release": "npx bumpp --commit --tag --push"
  },
  "dependencies": {
    "@vueuse/core": "^4.0.10",
    "@vueuse/head": "^0.2.3",
    "axios": "^0.21.1",
    "image-js": "^0.31.4",
    "mitt": "^2.1.0",
    "nprogress": "^0.2.0",
    "vue": "^3.0.5",
    "vue-i18n": "^9.0.0-rc.4",
    "vue-router": "^4.0.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.4.3",
    "@iconify/json": "^1.1.292",
    "@intlify/vite-plugin-vue-i18n": "^1.0.0-beta.12",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@vitejs/plugin-vue": "^1.1.2",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/server-renderer": "^3.0.5",
    "autoprefixer": "^10.2.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.18.0",
    "postcss": "^8.2.4",
    "postcss-nested": "^5.0.3",
    "tailwindcss": "^2.0.2",
    "typescript": "^4.1.3",
    "vite": "^2.0.0-beta.50",
    "vite-plugin-components": "^0.6.6",
    "vite-plugin-icons": "^0.2.1",
    "vite-plugin-pages": "^0.1.7",
    "vite-ssg": "^0.5.2"
  },
  "eslintConfig": {
    "extends": "@antfu/eslint-config",
    "ignorePatterns": [
      "dist/*",
      "**/vendor/*.js"
    ],
    "rules": {
      "no-unused-vars": "off",
      "no-cond-assign": [
        2,
        "except-parens"
      ],
      "@typescript-eslint/no-unused-vars": "off"
    }
  }
}
